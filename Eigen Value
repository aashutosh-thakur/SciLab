clc;clear;
A5=[2 0 0;0 2 0;0 0 2]
disp(spec(A5))
[c d]=spec(A5)
disp(c,d)
A6=[1 2 3;4 4 3;2 2 4]
disp(spec(A6))
disp(real(spec(A6)))
[c1,d1]=spec(A6)
disp(c1,d1)
disp(real(c1))
